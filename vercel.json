  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "ALPHA_VANTAGE_API_KEY": "@alpha-vantage-api-key",
    "NEWS_API_KEY": "@news-api-key"
  }
}
